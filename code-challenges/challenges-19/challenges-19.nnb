            "language": "javascript",
            "source": [
                "function timeInWords(h, m) {\n\n    let words = [\n        \"zero\", \"one\", \"two\", \"three\", \"four\", \"five\", \"six\", \"seven\", \"eight\", \"nine\", \"ten\", \"eleven\", \"twelve\", \"thirteen\", \"fourteen\", \"fifteen\", \"sixteen\", \"seventeen\", \"eightteen\", \"nineteen\", \"twenty\", \"twenty one\", \"twenty two\", \"twenty three\", \"twenty four\", \"twenty five\", \"twenty six\", \"twenty seven\", \"twenty eight\", \"twenty nine\"];\n\n    return !m\n        ? `${words[h]} o' clock`\n        : `${!(m % 30)\n            ? \"half\"\n            : !(m % 15)\n                ? \"quarter\"\n                : `${m <= 30 ? words[m] : words[60 - m]} ${`minute${m > 1 ? \"s\" : \"\"\n                }`}`\n        } ${m <= 30 ? \"past\" : \"to\"} ${words[m <= 30 ? h : h + 1]}`;\n\n}"
            ],
